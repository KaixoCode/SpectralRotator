{
  
  // ------------------------------------------------

  theme-name: Default
  
  // ------------------------------------------------

  variables: {

    // ------------------------------------------------

    $main-font: {
      map: map.png
      description: map.json
    }

    // ------------------------------------------------

  }
  
  // ------------------------------------------------

  font: $main-font

  background: {
    image: {
      source: background.png
      edges: [4, 28, 261, 8]
    }
  }

  button: {
    background-color: {
        value: [50, 50, 50]
        transition: 50
        
        hovering: [70, 70, 70]
        pressed: [90, 90, 90]
      }
  }

  spectrum: {
    color1: [0, 0, 0]
    color2: [60, 60, 179]
    color3: [239, 15, 0]
    color4: [255, 255, 64]
    color5: [255, 255, 255]
  }

  rotator: {
    background: {
      image: {
        source: rotator-background.png
        edges: [104, 28, 4, 4]
      }
    }
  }

  // ------------------------------------------------

}